{
  "name": "acf-options_page",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@dhruv-techapps/acf-common": "^1.0.12",
    "@dhruv-techapps/core-common": "^1.1.57",
    "@dhruv-techapps/core-components": "^1.0.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.5.3",
    "bootstrap-icons": "^1.0.0",
    "env-cmd": "^10.1.0",
    "react": "^16.14.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.9.5",
    "react-scripts": "3.4.3",
    "react-table": "^7.6.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.dev react-scripts start",
    "beta": "env-cmd -f .env.beta react-scripts start",
    "stable": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --format codeframe --ext mjs,jsx,js src",
    "build:stable": "react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:beta": "env-cmd -f .env.beta react-scripts build",
    "deploy:stable": "npm run build:stable && aws s3 sync build/. s3://stable.getautoclicker.com/",
    "deploy:dev": "npm run build:dev && aws s3 sync build/. s3://dev.getautoclicker.com/",
    "deploy:beta": "npm run build:beta && aws s3 sync build/. s3://beta.getautoclicker.com/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.1",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2"
  }
}
