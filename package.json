{
  "name": "acf-options_page",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@dhruv-techapps/acf-common": "^4.x",
    "@dhruv-techapps/core-common": "^4.x",
    "@dhruv-techapps/core-services": "^4.x",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.5.0",
    "env-cmd": "^10.1.0",
    "firebase": "^9.1.3",
    "i18next": "^20.6.1",
    "i18next-http-backend": "^1.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.4",
    "react-i18next": "^11.12.0",
    "react-reorder": "^3.0.0-alpha.7",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-table": "^7.7.0",
    "sitemap": "^7.0.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.localhost react-scripts start",
    "dev": "env-cmd -f .env.dev react-scripts start",
    "beta": "env-cmd -f .env.beta react-scripts start",
    "stable": "react-scripts start",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:beta": "env-cmd -f .env.beta react-scripts build",
    "build:stable": "react-scripts build",
    "lint": "eslint --cache --format codeframe --ext jsx,js,json src --fix",
    "sitemap:dev": "node sitemap-generator.js https://dev.getautoclicker.com/",
    "sitemap:beta": "node sitemap-generator.js https://beta.getautoclicker.com/",
    "sitemap:stable": "node sitemap-generator.js https://stable.getautoclicker.com/",
    "deploy:dev": "npm run sitemap:dev && npm run build:dev && aws s3 sync build/. s3://dev.getautoclicker.com/ --delete ",
    "deploy:beta": "npm run sitemap:beta && npm run build:beta && aws s3 sync build/. s3://beta.getautoclicker.com/ --delete ",
    "deploy:stable": "npm run sitemap:stable && npm run build:stable && aws s3 sync build/. s3://stable.getautoclicker.com/ --delete "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1"
  }
}
