{
  "name": "acf-options_page",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@dhruv-techapps/acf-common": "^3.1.2",
    "@dhruv-techapps/core-common": "^3.0.0",
    "@dhruv-techapps/core-components": "^3.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.3.0",
    "env-cmd": "^10.1.0",
    "firebase": "^8.2.7",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.15.1",
    "react-reorder": "^3.0.0-alpha.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-table": "^7.6.3"
  },
  "scripts": {
    "start": "env-cmd -f .env.localhost react-scripts start",
    "dev": "env-cmd -f .env.dev react-scripts start",
    "beta": "env-cmd -f .env.beta react-scripts start",
    "stable": "react-scripts start",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:beta": "env-cmd -f .env.beta react-scripts build",
    "build:stable": "react-scripts build",
    "deploy:dev": "npm run build:dev && aws s3 sync build/. s3://dev.getautoclicker.com/ --exclude=\"*.svg\" --exclude=\"*.json\" --exclude=\"*.txt\" --exclude=\"firebase-messaging-sw.js/*\" ",
    "deploy:beta": "npm run build:beta && aws s3 sync build/. s3://beta.getautoclicker.com/ --delete --size-only",
    "deploy:stable": "npm run build:stable && aws s3 sync build/. s3://stable.getautoclicker.com/ --delete ",
    "lint": "eslint --cache --format codeframe --ext mjs,jsx,js src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2"
  }
}
